            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"My AI Assessment Project\",\n  \"description\": \"Comprehensive AI maturity assessment for our organization\",\n  \"aiSystemType\": \"Machine Learning\"\n}"
